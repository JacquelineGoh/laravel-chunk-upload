{
  "name": "kladislav/laravel-chunk-upload",
  "description": "Service for chunked upload with several js providers",
  "license": "MIT",
  "keywords": [
    "laravel",
    "chunk",
    "upload"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Vladislav Kim",
      "email": "kim@mediacube.network"
    }
  ],
  "require": {
     "php": ">=8.1",
     "illuminate/support": "^10.0 || ^11.0",
     "illuminate/http": "^10.0 || ^11.0",
     "illuminate/console": "^10.0 || ^11.0",
     "illuminate/filesystem": "^10.0 || ^11.0"
   },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Kladislav\\LaravelChunkUpload\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kladislav\\LaravelChunkUpload\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kladislav\\LaravelChunkUpload\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
